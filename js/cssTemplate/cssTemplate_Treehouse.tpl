	color: {Log_in_gadgets-Links_on_hover-Font_color};
}






.WaGadgetBreadcrumbs ul li
{
	font-family: {Breadcrumbs-General-Font_family};
	font-size: {Breadcrumbs-General-Font_size};
	font-weight: {Breadcrumbs-General-Font_weight};
	font-style: {Breadcrumbs-General-Font_style};
	text-decoration: {Breadcrumbs-General-Text_decoration};
}
.WaGadgetBreadcrumbs ul li a,
.WaGadgetBreadcrumbs ul li a:link,
.WaGadgetBreadcrumbs ul li a:visited,
.WaGadgetBreadcrumbs ul li a:active
{
	color: {Breadcrumbs-Link-Font_color};
}
.WaGadgetBreadcrumbs ul li a:hover
{
	color: {Breadcrumbs-Link_on_hover-Font_color};
}
.WaGadgetBreadcrumbs ul li.last
{
	color: {Breadcrumbs-Current_page-Font_color};
}





.WaGadgetNavigationLinks a,
.WaGadgetNavigationLinks a:link,
.WaGadgetNavigationLinks a:visited,
.WaGadgetNavigationLinks a:active
{
	color: {Navigation_links-Link-Font_color};
}
.WaGadgetNavigationLinks a:hover
{
	color: {Navigation_links-Link_on_hover-Font_color};
}


























